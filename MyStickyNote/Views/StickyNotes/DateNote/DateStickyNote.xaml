<UserControl x:Class="MyStickyNote.Views.StickyNotes.DateStickyNote"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MyStickyNote.Views.StickyNotes"
             xmlns:date="clr-namespace:MyStickyNote.Views.StickyNotes.DateNote"
             mc:Ignorable="d" 
             d:DesignHeight="500" d:DesignWidth="500">
    <Grid>
        <local:StickyNoteContainer x:Name="NoteTopPart">
            <local:StickyNoteContainer.TextContent>
                <date:NoteDatePicker></date:NoteDatePicker>
            </local:StickyNoteContainer.TextContent>
        </local:StickyNoteContainer>
    </Grid>
</UserControl>
