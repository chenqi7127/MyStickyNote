        xmlns:local="clr-namespace:MyStickyNote"
        xmlns:control="clr-namespace:MyStickyNote.MyControls"
        mc:Ignorable="d"
        Title="MainWindow" Background="Transparent" Height="350" Width="525" WindowStyle="None" WindowState="Maximized" AllowsTransparency="True">
    <Grid x:Name="NotesGrid">
        <control:StickyNote_UC x:Name="test" GotMouseCapture="StickyNote_UC_GotMouseCapture" Margin="200,0,0,0"></control:StickyNote_UC>
        <control:StickyNote_UC  x:Name="test2" GotMouseCapture="StickyNote_UC_GotMouseCapture" ></control:StickyNote_UC>
    </Grid>
